<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Skale_W_Praktyce.Views.Flyout.ScalesListPage_Flyout"
             xmlns:flyoutpages="clr-namespace:Skale_W_Praktyce.Views.Flyout"
             xmlns:pages="clr-namespace:Skale_W_Praktyce.Views"
             NavigationPage.HasNavigationBar="False">
    <FlyoutPage.Resources>
        <ResourceDictionary>

        </ResourceDictionary>
    </FlyoutPage.Resources>
    <FlyoutPage.Flyout>
        <flyoutpages:FlyoutMenuPage x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <pages:ScalesListPage />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>