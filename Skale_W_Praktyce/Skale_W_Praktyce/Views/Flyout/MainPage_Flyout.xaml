<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Skale_W_Praktyce.Views.Flyout.MainPage_Flyout"
             xmlns:flyoutpages="clr-namespace:Skale_W_Praktyce.Views.Flyout"
             xmlns:pages="clr-namespace:Skale_W_Praktyce.Views"
             NavigationPage.HasNavigationBar="False">
  <FlyoutPage.Flyout>
    <flyoutpages:MainPage_FlyoutMenuPage x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:MainPage />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>